{
  "name": "widl-spec",
  "description": "WIDL AST nodes",
  "author": "jsoverson@gmail.com",
  "version": "0.0.0",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "codegen:rust": "ts-node src/bin/generate-rust-ast.ts widl.idl > generated/ast-nodes.rs",
    "codegen:typescript": "ts-node src/bin/generate-typescript-ast.ts widl.idl attribute-order.json > generated/ast-nodes.ts",
    "build:json": "ts-node src/bin/generate-json.ts widl.idl > widl-spec.json",
    "build": "tsc --declaration",
    "compile": "npm run clean && npm run build",
    "clean": "rimraf dist",
    "lint": "",
    "prepublishOnly": "npm run compile",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "watch": "npm run clean && tsc -w --declaration",
    "test:unit": "mocha",
    "test": "npm run lint && npm run test:unit"
  },
  "mocha": {
    "extends": "etc/.mocharc.json"
  },
  "prettier": "./etc/.prettierrc.json",
  "eslintConfig": {
    "extends": "./etc/.eslintrc.json"
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/find-root": "^1.1.2",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.6.2",
    "@types/webidl2": "^23.13.5",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "chai": "^4.3.4",
    "eslint": "^7.26.0",
    "mocha": "^8.4.0",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webidl2": "^24.1.2"
  },
  "dependencies": {}
}
